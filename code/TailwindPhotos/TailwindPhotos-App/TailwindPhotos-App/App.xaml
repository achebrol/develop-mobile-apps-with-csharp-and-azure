<?xml version="1.0" encoding="UTF-8" ?>
<Application
    x:Class="Tailwind.Photos.App"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Application.Resources>
        <ResourceDictionary>
            <OnPlatform x:Key="FontAwesome" x:TypeArguments="x:String">
                <On Platform="iOS" Value="Font Awesome 5 Free"/>
                <On Platform="Android" Value="fa-solid-900.ttf#Font Awesome 5 Free Solid"/>
            </OnPlatform>

            <OnPlatform x:Key="Pacifico" x:TypeArguments="x:String">
                <On Platform="iOS" Value="Pacifico"/>
                <On Platform="Android" Value="Pacifico.ttf#Pacifico"/>
            </OnPlatform>

            <x:String x:Key="FA_UserCircle">&#xf2bd;</x:String>

            <Color x:Key="SplashBackground">#9DD3AF</Color>

        </ResourceDictionary>
    </Application.Resources>
</Application>